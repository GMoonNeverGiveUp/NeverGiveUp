{
  "name": "galactic-genius-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "workspaces": [
    "src",
    "server",
    "scripts"
  ],
  "scripts": {
    "bootstrap": "pnpm install --recursive",
    "build": "turbo run build --filter=...[HEAD]",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "tsc:check": "tsc -b --verbose",
    "tsc:watch": "tsc -b --watch",
    "check": "pnpm turbo run lint test typecheck",
    "lint:fix": "pnpm --filter src exec eslint . --ext .ts,.tsx,.js,.jsx,.astro --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "jest": "^30.0.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4"
  },
  "dependencies": {
    "astro": "^5.10.0",
    "glob": "^10.4.5"
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.4.5",
      "inflight": "npm:@esbuild/dummy-pkg"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
